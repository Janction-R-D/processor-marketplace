    "@rainbow-me/rainbowkit": "^2.1.2",
    "@tanstack/react-query": "^5.28.4",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "animate.css": "^4.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "ethers": "5",
    "framer-motion": "^12.5.0",
    "hover.css": "^2.3.2",
    "js-base64": "^3.7.7",
    "katex": "^0.16.11",
    "lodash": "^4.17.21",
