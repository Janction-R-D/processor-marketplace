    "papaparse": "^5.4.1",
    "rc-bullets": "^1.5.16",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-joyride": "^2.9.3",
    "react-lottie": "^1.2.4",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "remark-toc": "^9.0.0",
    "siwe": "^2.3.2",
    "swiper": "^11.1.15",
    "umi": "^3.5.41",
    "unified": "^11.0.5",
    "viem": "2.9.31",
